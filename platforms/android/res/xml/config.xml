<?xml version='1.0' encoding='utf-8'?>
<widget id="us.commgamers.sausage" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <name>Sausage</name>
    <description>
        Say Sausage.
    </description>
    <author email="bulbufet@gmail.com" href="http://commgamers.us">
        Bulbufet
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="permissions" value="none" />
    <icon density="ldpi" platform="android" src="www/img/mipmap-hdpi/ic_launcher.png" />
    <icon density="mdpi" platform="android" src="www/img/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" platform="android" src="www/img/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xhdpi" platform="android" src="www/img/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xhdpi" platform="android" src="www/img/mipmap-xxxhdpi/ic_launcher.png" />
</widget>
